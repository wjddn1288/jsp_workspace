<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>GalleryProject</display-name>
  
  <servlet>
  <servlet-name>uploadServlet</servlet-name>
  <servlet-class>gallery.servlet.UploadServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>uploadServlet</servlet-name>
 		<url-pattern>/gallery/upload</url-pattern>
 	</servlet-mapping>
 	
 	 <servlet>
  <servlet-name>deleteServlet</servlet-name>
  <servlet-class>gallery.servlet.DeleteServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>deleteServlet</servlet-name>
 		<url-pattern>/gallery/del</url-pattern>
 	</servlet-mapping>
 	
 	 	 <servlet>
  <servlet-name>editServlet</servlet-name>
  <servlet-class>gallery.servlet.EditServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>editServlet</servlet-name>
 		<url-pattern>/gallery/edit</url-pattern>
 	</servlet-mapping>
 	
 	 	 	 <servlet>
  <servlet-name>updateServlet</servlet-name>
  <servlet-class>gallery.servlet.UpdateServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>updateServlet</servlet-name>
 		<url-pattern>/gallery/update</url-pattern>
 	</servlet-mapping>
 	
  <servlet>
	  <servlet-name>registServlet</servlet-name>
	  <servlet-class>member.servlet.RegistServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>registServlet</servlet-name>
 		<url-pattern>/member/regist</url-pattern>
 	</servlet-mapping>
 	
	  <servlet>
	  <servlet-name>loginServlet</servlet-name>
	  <servlet-class>member.servlet.LoginServlet</servlet-class>
  </servlet>
  	
 	<servlet-mapping>
 		<servlet-name>loginServlet</servlet-name>
 		<url-pattern>/member/login</url-pattern>
 	</servlet-mapping>
 	
</web-app>